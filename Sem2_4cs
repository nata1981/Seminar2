// Напишите программу, которая на вход принимает натуральное число N, 
//а на выходе показывает его цифры через запятую.

  int n = int.Parse(Console.ReadLine());
            string s = n.ToString();
            string[] arr = s.Split(',');
            for (int i = 0; i < arr.Length; i++)
            {
                Console.Write(arr[i] + ",");